<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.baniimei">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_icon_100"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_icon_100"
        android:supportsRtl="true"
        android:theme="@style/splashScreenTheme">
        <activity android:name=".activitati.ChestionarActivity"></activity>
        <activity
            android:name=".activitati.SetariActivity"
            android:parentActivityName=".activitati.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activitati.ChestionarCapitoleActivity"
            android:parentActivityName=".activitati.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activitati.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET"/>

</manifest>